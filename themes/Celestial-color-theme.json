      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "fontStyle": "",
        "foreground": "#d864d8"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "fontStyle": "",
        "foreground": "#f76190"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "",
        "foreground": "#58B896"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF7F9D"
