      }
    },
    {
      "name": "By uonick",
      "settings": {
        "background": "#202025ff",
        "foreground": "#c5c8c6ff"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "background": "#1e1e1e",
        "foreground": "#C5C8C6"
      }
